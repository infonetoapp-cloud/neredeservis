{
    "functions":  {
                      "source":  "functions"
                  },
    "firestore":  {
                      "rules":  "firestore.rules",
                      "indexes":  "firestore.indexes.json"
                  },
    "database":  {
                     "rules":  "database.rules.json"
                 },
    "storage":  {
                    "rules":  "storage.rules"
                },
    "hosting":  [
                    {
                        "target":  "app",
